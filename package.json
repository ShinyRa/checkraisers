{
	"name": "poker-app",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-check && svelte-kit dev",
		"build": "svelte-kit build",
		"compileSocket": "tsc ./lib/backend/socketServer/server.ts",
		"clean": "rm -rf node_modules && rm -rf .svelte-kit && rm -rf build",
		"preview": "svelte-kit preview",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "jest frontend --passWithNoTests && jest backend --passWithNoTests",
		"test:frontend": "jest lib/logic/frontend --passWithNoTests",
		"test:backend": "jest lib/backend --passWithNoTests",
		"test:coverage": "jest --passWithNoTests --coverage",
		"depcheck": "depcheck",
		"test:client": "jest --detectOpenHandles Client.test.ts",
		"test:crud": "jest --detectOpenHandles Crud.test.ts"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.16.11",
		"@sveltejs/adapter-node": "^1.0.0-next.70",
		"@sveltejs/adapter-static": "^1.0.0-next.26",
		"@sveltejs/kit": "^@sveltejs/kit@1.0.0-next.287",
		"@testing-library/svelte": "^3.0.3",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.4.0",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"depcheck": "^1.4.3",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"jest": "^27.5.1",
		"prettier": "^2.4.1",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-jester": "^2.3.1",
		"svelte-preprocess": "^4.10.2",
		"ts-jest": "^27.1.3",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"@types/jsonwebtoken": "^8.5.8",
		"@types/socket.io-client": "^3.0.0",
		"cookie": "^0.5.0",
		"dotenv": "^14.2.0",
		"global": "^4.4.0",
		"jsonwebtoken": "^8.5.1",
		"mongodb": "^4.4.0",
		"sass": "^1.49.7",
		"scandir": "^0.1.2",
		"socket.io": "^4.5.0",
		"socket.io-client": "^4.5.0"
	}
}
